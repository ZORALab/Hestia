{{- /* INPUT PARAMETERS */ -}}
{{- /* . = Page data in Hestia Structure */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := dict -}}
{{- $ret := false -}}




{{- /* merge page title data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_title" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* merge page description data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_description" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* merge page format data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_format" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* merge page keywords data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_keywords" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* merge page redirect data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_redirect" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* merge page alternate languages data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_languages" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* merge page url data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_url" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* merge page thumbnails data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_thumbnails" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* merge page locale data */ -}}
{{- $ret = partial "Hestia/functions/render/json/meta/_locale" . -}}
{{- $dataList = merge $dataList $ret -}}




{{- /* render output */ -}}
{{- return dict "Page" $dataList -}}
