{{- /* INPUT PARAMETERS */ -}}
{{- /* . = Page data in Hestia Structure  */ -}}
{{- /* .Input.Width = image Width dimension */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := 0 -}}
{{- $ret := false -}}




{{- $ret = merge . (dict "Input" (dict "Query" .Input.Width)) -}}
{{- $ret = partial "Hestia/functions/data/scan/GoTemplateFunction" $ret -}}
{{- $ret = partial "Hestia/functions/data/trim/Whitespace" $ret -}}
{{- $ret = merge . (dict "Input" (dict "Dimension" $ret)) -}}
{{- $dataList = partial "Hestia/functions/data/dimension/Length" $ret -}}




{{- /* render output */ -}}
{{- return $dataList -}}
