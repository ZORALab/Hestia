{{- /* INPUT PARAMETERS */ -}}
{{- /* . = input for checking */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := false -}}




{{- /* check given data is an URL */ -}}
{{- if strings.HasSuffix (printf "%T" .) "URL" -}}
	{{- $dataList = true -}}
{{- end -}}




{{- /* render output */ -}}
{{- return $dataList -}}
