{{- /* INPUT PARAMETERS */ -}}
{{- /* .  = Hugo data structure  */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := dict -}}
{{- $ret := false -}}




{{- /* process Title.Page */ -}}
{{- $ret = partial "Hestia/functions/data/trim/Whitespace" .Page.Title -}}
{{- $dataList = merge $dataList (dict "Page" $ret) -}}




{{- /* process Title.Site */ -}}
{{- $ret = .Page.Lang -}}
{{- range $v := .Site.Languages -}}
	{{- if eq $v.Lang $ret -}}
		{{- $ret = $v.Title -}}
	{{- end -}}
{{- end -}}
{{- if eq $ret .Page.Lang -}}
	{{- $ret = .Site.Title -}}
{{- end -}}
{{- $ret = partial "Hestia/functions/data/trim/Whitespace" $ret -}}
{{- $dataList = merge $dataList (dict "Site" $ret) -}}




{{- /* render output */ -}}
{{- return $dataList -}}
