{{- /* INPUT PARAMETERS */ -}}
{{- /* . = string for trimming */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := . -}}




{{- /* trim both sides */ -}}
{{- $dataList = partial "Hestia/functions/data/string/TrimWhitespaceLeft"
	. -}}
{{- $dataList = partial "Hestia/functions/data/string/TrimWhitespaceRight"
	$dataList -}}




{{- /* render output */ -}}
{{- return $dataList -}}
