{{- /* INPUT PARAMETERS */ -}}
{{- /* . = string for trimming */ -}}

{{- /* CONSTANTS */ -}}
{{- $CHARSET := partial "Hestia/functions/data/string/CONST_WHITESPACE" . -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := . -}}




{{- /* trim right side */ -}}
{{- if partial "Hestia/functions/data/string/IsString" . -}}
	{{- $dataList = strings.TrimRight $CHARSET . -}}
{{- end -}}




{{- /* render output */ -}}
{{- return $dataList -}}
