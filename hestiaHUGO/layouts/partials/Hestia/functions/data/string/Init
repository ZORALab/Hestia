{{- /* INPUT PARAMETERS */ -}}
{{- /* . = Page data in Hestia Structure */ -}}
{{- /* .Input.Data = string for processing */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $Page := partial "Hestia/functions/data/Clean" . -}}
{{- $dataList := dict -}}
{{- $console := dict -}}
{{- $ret := false -}}




{{- /* standardize string data */ -}}
{{- $ret = merge . (dict "Input" (dict "Query" ".Input.Data")) -}}
{{- $ret = partial "Hestia/functions/data/Get" $ret -}}
{{- if not $ret -}}
	{{- $ret = "" -}}
{{- end -}}


{{- if partial "Hestia/functions/data/string/IsString" $ret -}}
	{{- $ret = merge $Page (dict "Input" (dict "Data" $ret)) -}}
	{{- $ret = partial "Hestia/functions/data/string/_processGoTemplates"
		$ret -}}
	{{- if $ret.Console -}}
		{{- $console = printf "string: %s" $ret.Console.Message -}}
		{{- $console = dict "Message" $console -}}
	{{- end -}}
	{{- $dataList = $ret.Output -}}
{{- else -}}
	{{- $dataList = "" -}}
	{{- $console = printf "string: given data is not a string: '%v'\n"
		.Input.Data -}}
	{{- $console = dict "Message" $console -}}
{{- end -}}




{{- /* render output */ -}}
{{- return dict "Output" $dataList "Console" $console -}}
