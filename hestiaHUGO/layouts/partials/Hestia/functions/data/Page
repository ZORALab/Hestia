{{- /* INPUT PARAMETERS */ -}}
{{- /* .    = hugo page data context */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := dict "IsHestia" true -}}
{{- $ret := false -}}




{{- /* prepare Hestia .Page.Format */ -}}
{{- $ret = partial "Hestia/functions/data/hugo/ParseFormat" . -}}
{{- $dataList = merge $dataList (dict "Format" $ret) -}}




{{- /* prepare Hestia .Page.Languages */ -}}
{{- $ret = partial "Hestia/functions/data/hugo/ParseLanguages" . -}}
{{- $dataList = merge $dataList (dict "Languages" $ret) -}}




{{- /* prepare Hestia .Page.Titles */ -}}
{{- $ret = partial "Hestia/functions/data/hugo/ParseTitle" . -}}
{{- $dataList = merge $dataList (dict "Titles" $ret) -}}




{{- /* prepare Hestia .Page.URL */ -}}
{{- $ret = partial "Hestia/functions/data/hugo/ParseURL" . -}}
{{- $dataList = merge $dataList (dict "URL" $ret) -}}




{{- /* prepare Hestia .Page.Content */ -}}
{{- $ret = partial "Hestia/functions/data/hugo/ParseContent" . -}}
{{- $dataList = merge $dataList (dict "Content" $ret) -}}




{{- /* prepare Hestia .Page.Data */ -}}
{{- $ret = partial "Hestia/functions/data/hugo/ParseDatastore" . -}}
{{- $dataList = merge $dataList (dict "Data" $ret) -}}




{{- /* prepare Hestia .Page.Filesystem */ -}}
{{- $ret = partial "Hestia/functions/data/hugo/ParseFS" . -}}
{{- $dataList = merge $dataList (dict "Filesystem" $ret) -}}




{{- /* prepare Hestia .Page.Thumbnails */ -}}
{{- $ret = partial "Hestia/functions/data/hugo/ParseThumbnails" . -}}
{{- $dataList = merge $dataList (dict "Thumbnails" $ret) -}}




{{- /* BEGIN POST PROCESSING - Hugo interfaces ended beyond this line */ -}}
{{- $dataList = partial "Hestia/functions/data/post-processing/Thumbnails" $dataList -}}



{{- /* render output */ -}}
{{- return $dataList -}}
