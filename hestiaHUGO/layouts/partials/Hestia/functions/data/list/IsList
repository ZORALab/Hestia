{{- /* INPUT PARAMETERS */ -}}
{{- /* . = input for checking */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := false -}}




{{- /* check given data is a slice or an array */ -}}
{{- $dataList = partial "Hestia/functions/data/list/IsSlice" . -}}




{{- /* otherwise, check given data is a map */ -}}
{{- if not $dataList -}}
	{{- $dataList = partial "Hestia/functions/data/list/IsMap" . -}}
{{- end -}}




{{- /* render output */ -}}
{{- return $dataList -}}
