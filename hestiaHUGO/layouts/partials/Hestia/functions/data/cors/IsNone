{{- /* INPUT PARAMETERS */ -}}
{{- /* . = Page data in Hestia Structure */ -}}
{{- /* .Input.CORS = CORS data for validation (string) */ -}}




{{- /* prepare working variables for this function */ -}}
{{- $dataList := false -}}




{{- /* validate data type */ -}}
{{- $dataList = partial "Hestia/functions/data/cors/_toType" . -}}
{{- if not $dataList -}}
	{{- $dataList = true -}}
{{- else -}}
	{{- $dataList = false -}}
{{- end -}}




{{- /* render output */ -}}
{{- return $dataList -}}
