{{- /* INPUT PARAMETERS */ -}}
{{- /* . = Page Hugo/Hestia Data Structure */ -}}
{{- /* .Output = Clean scratchpad for saving output (scratchPad) */ -}}

{{- /* OUTPUT */ -}}
{{- /* "Value" = The processed output (any) */ -}}




{{- define "hestiaHUGO.Clean" -}}
	{{- /* prepare working variables for this function */ -}}
	{{- $Output := index . "Output" -}}
	{{- $Page := merge . (dict "Input" false "Console" false "Output" false) -}}




	{{- /* clean up data structure */ -}}
	{{- $Page = merge $Page (dict "Input" dict "Console" "" "Output" newScratch) -}}




	{{- /* save to output */ -}}
	{{- $Output.Set "Value" $Page -}}
{{- end -}}
