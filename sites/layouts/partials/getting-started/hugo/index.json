{{- /* prepare variables for function */ -}}
{{- $Page := partial "Hestia/functions/data/Clean" . -}}
{{- $dataList := dict -}}
{{- $list := slice -}}
{{- $thumbnails := slice -}}
{{- $keys := .Data.Page.i18n -}}
{{- $data := dict -}}
{{- $ret := false -}}




{{- /* execute functon */ -}}
{{- $dataList = merge $dataList (dict $keys.Title .Titles.Page) -}}
{{- $dataList = merge $dataList (dict $keys.Pitch .Descriptions.Page.Pitch) -}}


{{- $list = slice -}}
{{- range $i, $v := .Data.Page.Guides.User -}}
	{{- $data = dict $keys.Name $v.Name -}}


	{{- $ret = strings.TrimRight "\n" $v.Pitch -}}
	{{- $data = merge $data (dict $keys.Pitch $ret) -}}


	{{- $thumbnails = slice -}}
	{{- $ret = printf "%s/thumbnails-240x240.avif" $v.ID -}}
	{{- $ret = merge $Page (dict "Input" (dict "URL" $ret )) -}}
	{{- $ret = partial "Hestia/functions/data/url/Init" $ret -}}
	{{- $ret = string $ret.Output }}
	{{- $ret = dict "URL" $ret "Type" "image/avif" "Width" "240px" "Height" "240px" -}}
	{{- $thumbnails = append $ret $thumbnails -}}

	{{- $ret = printf "%s/thumbnails-240x240.webp" $v.ID -}}
	{{- $ret = merge $Page (dict "Input" (dict "URL" $ret )) -}}
	{{- $ret = partial "Hestia/functions/data/url/Init" $ret -}}
