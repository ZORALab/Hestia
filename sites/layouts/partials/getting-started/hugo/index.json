	{{- $ret = dict "URL" $ret "Type" "image/jpeg" "Width" "1200px" "Height" "630px" -}}
	{{- $thumbnails = append $ret $thumbnails -}}
	{{- $data = merge $data (dict $keys.Thumbnails $thumbnails) -}}

	{{- $ret = merge $Page (dict "Input" (dict "URL" $v.ID )) -}}
	{{- $ret = partial "Hestia/functions/data/url/Init" $ret -}}
	{{- $ret = string $ret.Output }}
	{{- $data = merge $data (dict $keys.URL $ret) -}}


	{{- $list = append $data $list -}}
{{- end -}}
{{- $dataList = merge $dataList (dict $keys.Guides (dict $keys.User $list)) -}}




