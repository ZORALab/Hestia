	{{- $ret = dict "URL" $ret "Type" "image/webp" "Width" "240px" "Height" "240px" -}}
	{{- $thumbnails = append $ret $thumbnails -}}

	{{- $ret = printf "%s/thumbnails-240x240.jpg" $v.ID -}}
	{{- $ret = merge $Page (dict "Input" (dict "URL" $ret )) -}}
	{{- $ret = partial "Hestia/functions/data/url/Init" $ret -}}
	{{- $ret = string $ret.Output }}
	{{- $ret = dict "URL" $ret "Type" "image/jpeg" "Width" "240px" "Height" "240px" -}}
