	{{- $ret = dict "URL" $ret "Type" "image/jpeg" "Width" "480px" "Height" "480px" -}}
	{{- $thumbnails = append $ret $thumbnails -}}

	{{- $ret = printf "%s/thumbnails-1200x1200.jpg" $v.ID -}}
