{{- /*
Page's JSON Output Prime Control

This file is your JSON output file (index.html). Hugo's and Go's template
processors are available at your disposal in case of mathematical or logical
algorithms development.
*/ -}}
{{- /* prepare variables for function */ -}}
{{- $Page := . -}}
{{- $dataList := dict -}}
{{- $i18n := .Data.Page.i18n -}}
{{- $lang := dict -}}
{{- $list := slice -}}
{{- $ret := false -}}




{{- /* execute function */ -}}
{{- $dataList = merge $dataList (dict
	$i18n.Title .Titles.Page
	$i18n.Description (printf "%s %s" .Descriptions.Page.Pitch .Descriptions.Page.Summary)
	$i18n.Labels.Contributors $Page.Contributors
) -}}


{{- $list = slice -}}
{{- range $i, $v := $Page.Data.Store.Business -}}
	{{- $lang = index $v.Metadata.Languages $Page.Languages.Current.ID -}}
	{{- $list = append
		(dict
			$i18n.Labels.Reason $lang.Name
			$i18n.Labels.Value $lang.Descriptions
		)
	$list -}}
{{- end -}}
{{- $dataList = merge $dataList (dict $i18n.WhyItMatters.Title $list) -}}


{{- $lang = index $Page.Data.Store.Programming.Hugo.Metadata.Languages
	$Page.Languages.Current.ID -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Language $lang.Title
	$i18n.Labels.Strengths $lang.Strengths
	$i18n.Labels.Features $lang.Features
))) -}}

{{- $list = slice -}}
{{- range $i, $v := $lang.Strengths -}}
	{{- $list = append $v.Pitch $list -}}
{{- end -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Strengths $list
))) -}}

{{- $list = slice -}}
{{- range $i, $v := $lang.Features -}}
	{{- $list = append $v.Pitch $list -}}
{{- end -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Features $list
))) -}}


{{- $lang = index $Page.Data.Store.Programming.Go.Metadata.Languages
	$Page.Languages.Current.ID -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Strengths $lang.Strengths
	$i18n.Labels.Features $lang.Features
))) -}}

{{- $list = slice -}}
{{- range $i, $v := $lang.Strengths -}}
	{{- $list = append $v.Pitch $list -}}
{{- end -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Strengths $list
))) -}}

{{- $list = slice -}}
{{- range $i, $v := $lang.Features -}}
	{{- $list = append $v.Pitch $list -}}
{{- end -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Features $list
))) -}}


{{- $lang = index $Page.Data.Store.Programming.Nim.Metadata.Languages
	$Page.Languages.Current.ID -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Language $lang.Title
	$i18n.Labels.Strengths $lang.Strengths
	$i18n.Labels.Features $lang.Features
))) -}}

{{- $list = slice -}}
{{- range $i, $v := $lang.Strengths -}}
	{{- $list = append $v.Pitch $list -}}
{{- end -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Strengths $list
))) -}}

{{- $list = slice -}}
{{- range $i, $v := $lang.Features -}}
	{{- $list = append $v.Pitch $list -}}
{{- end -}}
{{- $dataList = merge $dataList (dict $i18n.Labels.Programming (dict $lang.Brand (dict
	$i18n.Labels.Features $list
))) -}}


{{- $list = slice -}}
{{- range $i, $v := $Page.Data.Store.Licenses -}}
	{{- $lang = index $v.Metadata.Languages $Page.Languages.Current.ID -}}
	{{- $dataList = merge $dataList (dict $i18n.Labels.Licenses (dict $v.Metadata.SPDX (dict
		$i18n.Labels.Name $lang.Name
		$i18n.Labels.Coverage $lang.Coverage
		$i18n.Labels.Files $v.Metadata.Files
	))) -}}
{{- end -}}




{{- /* render output */ -}}
{{- jsonify $dataList -}}
